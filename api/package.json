{
    "name": "sar-Management",
    "type": "module",
    "version": "2.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "test": "tape test/**.test.js",
        "build": "tsc --build && cp package.json ./dist/ && cp migrations/ dist/ -r && cp lib/user.webp dist/lib/",
        "lint": "eslint index.ts routes/ lib/",
        "loaddb": "npm run loaddbraw && echo \"UPDATE users SET email = username || '@ingalls.ca'\" | psql sar",
        "loaddbraw": "echo 'DROP DATABASE sar' | psql && echo 'CREATE DATABASE sar' | psql && psql sar < sar-latest.dump",
        "migratedb": "npm run migratedb-pre && drizzle-kit generate && npm run migratedb-commit",
        "migratedb-custom": "npm run migratedb-pre && drizzle-kit generate --custom && npm run migratedb-commit",
        "migratedb-pre": "npm run lint && git stash && tsc --outDir .",
        "migratedb-commit": "git add migrations/* && git commit -m 'Custom DB Migration' && git clean -f && git stash",
        "prod": "node dist/index.js --heartbeat",
        "dev": "StackName=test tsx watch --ignore web/ index.ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ingalls/SAR-Management.git"
    },
    "author": "Nick Ingalls <nick@ingalls.ca>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/ingalls/SAR-Management/issues"
    },
    "homepage": "https://github.com/ingalls/SAR-Management#readme",
    "dependencies": {
        "@aws-sdk/client-s3": "^3.267.0",
        "@aws-sdk/lib-storage": "^3.287.0",
        "@openaddresses/batch-error": "^2.0.0",
        "@openaddresses/batch-generic": "^24.9.0",
        "@openaddresses/batch-schema": "^10.0.0",
        "@sinclair/typebox": "^0.34.0",
        "@slack/web-api": "^7.13.0",
        "@supercharge/promise-pool": "^3.2.0",
        "ajv": "^8.11.2",
        "ajv-formats": "^3.0.0",
        "api2pdf": "^2.0.3",
        "bcrypt": "^6.0.0",
        "busboy": "^1.6.0",
        "chart.js": "^4.5.0",
        "connect-history-api-fallback": "^2.0.0",
        "cors": "^2.8.5",
        "csv-stringify": "^6.2.1",
        "discord.js": "^14.19.3",
        "drizzle-kit": "^0.31.0",
        "drizzle-orm": "^0.45.0",
        "drizzle-typebox": "^0.3.0",
        "express": "^5.0.0",
        "express-minify": "^1.0.0",
        "form-data": "^4.0.0",
        "ical-generator": "^10.0.0",
        "jsonwebtoken": "^9.0.0",
        "mailgen": "^2.0.27",
        "mailgun.js": "^12.0.3",
        "minimist": "^1.2.7",
        "moment": "^2.29.4",
        "moment-timezone": "^0.6.0",
        "otplib": "^13.1.1",
        "pg": "^8.8.0",
        "phone": "^3.1.45",
        "qrcode": "^1.5.4",
        "sharp": "^0.34.0",
        "swagger-ui-express": "^5.0.0",
        "vcard-creator": "^0.7.0"
    },
    "devDependencies": {
        "@types/express": "^5.0.0",
        "@types/geojson": "^7946.0.14",
        "@types/qrcode": "^1.5.6",
        "@types/swagger-ui-express": "^4.1.3",
        "eslint": "^9.0.0",
        "nodemon": "^3.0.1",
        "path-to-regexp": "^8.0.0",
        "tape": "^5.6.1",
        "tsx": "^4.15.6",
        "typescript": "^5.3.3",
        "typescript-eslint": "^8.0.0"
    }
}
